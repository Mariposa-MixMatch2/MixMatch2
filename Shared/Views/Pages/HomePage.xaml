<?xml version="1.0" encoding="utf-8"?>

<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:MixMatch2.Shared.Views.Pages"
    xmlns:helpers="clr-namespace:MixMatch2.Resources.Helpers"
    xmlns:vm="clr-namespace:MixMatch2.Shared.ViewModels"
    x:Class="MixMatch2.Shared.Views.Pages.HomePage">
    <ContentPage.BindingContext>
        <vm:HomeViewModel />
    </ContentPage.BindingContext>

    <Grid
        x:Name="HomeGrid"
        ColumnDefinitions="{Binding HomeGridColumnDefinitions}"
        RowDefinitions="{Binding HomeGridRowDefinitions}">
        
        <Label Grid.Row="5" Grid.Column="5">
            Home Page
        </Label>
    </Grid>
</ContentPage>